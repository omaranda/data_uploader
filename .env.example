# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=data_uploader
DB_USER=uploader
DB_PASSWORD=uploader_pass

# Application Configuration
LOG_LEVEL=INFO

# API Configuration
# Required: Your MadXXX API key for authentication
MADXXX_API_KEY=your-api-key-here

# Required: MadXXX API endpoint URL
MADXXX_API_URL=https://your-madxxx-api-endpoint.com/api/v1/madxxx_tasks/job

# Optional: Timeout for API requests in seconds
MADXXX_API_TIMEOUT=30

# MadXXX Job Configuration
# Backend function to call for audio file registration
MADXXX_FUNCTION_NAME=register_audiofiles
# Python module path on backend
MADXXX_MODULE_NAME=madxxx_workbench.audio.register_audio
# SQS queue name for job processing
MADXXX_SQS_QUEUE=madXXX_tasks_data_registration
# AWS region for SQS queue
MADXXX_SQS_REGION=eu-west-1